<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}IMDER - Bienvenido{% endblock %}</title>
        
        {# <link rel="stylesheet" href="css/style.css" type="text/css" media="all" /> #}

        {# CSS plantilla #}
        <link href="{{ asset('css/pixel/style.css') }}" rel="stylesheet" />
        {# <link href="{{ asset('css/bootstrap.min.css') }}" rel="stylesheet" /> #}
                     
        {# CSS galeria de imagenes #}
        <link href="{{ asset('css/least/least.min.css') }}" rel="stylesheet" />

        {# CSS menu drill #}
        <link href="{{ asset('css/drill/dcdrilldown.css') }}" rel="stylesheet" />
        <link href="{{ asset('css/drill/skins/graphite.css') }}" rel="stylesheet" />
        <link href="{{ asset('css/drill/skins/blue.css') }}" rel="stylesheet" />
        <link href="{{ asset('css/drill/skins/grey.css') }}" rel="stylesheet" />
        <link href="{{ asset('css/drill/skins/demo.css') }}" rel="stylesheet" />
    </head>

    <body>
        {% block body %}
            {% block menu %}{% endblock %}
            {% block slider %}{% endblock %}
            {% block menucito %}{% endblock %}
            {% block texto %}{% endblock %}
            {% block barra1 %}{% endblock %}
            {% block barra2 %}{% endblock %}
            {% block sidebar %}{% endblock %}
            {% block footer %}{% endblock %}
        {% endblock %}
        {# fin block body #}

        {# JS aplicacion basicas #}
        <script src="{{ asset('js/jquery.js')     }}" type="text/javascript"></script>
        <script src="{{ asset('js/utilidades.js') }}" type="text/javascript"></script>

       {# Variables de AJAX #}
        <script type="text/javascript">
            var pruebaAjax     = '{{path('pruebaAjax')}}';
            var agregarAjax    = '{{path('agregarAjax')}}';
            var editarProyecto = '{{path('editarProyecto')}}';
            var editarAjax     = '{{path('editarAjax')}}';
            var eliminarAjax   = '{{path('eliminarAjax')}}';
            var listar         = '{{path('listar')}}';
        </script> 

        {# JS menu drill #}
        <script src="{{ asset('js/drill/jquery.cookie.js')     }}" type="text/javascript"></script>
        <script src="{{ asset('js/drill/jquery.dcdrilldown.1.2.min.js')     }}" type="text/javascript"></script>
        <script src="{{ asset('js/drill/accion.js')     }}" type="text/javascript"></script>

        {# JS galeria de imagenes #}
        <script src="{{ asset('js/least/least.min.js')     }}" type="text/javascript"></script>
        <script src="{{ asset('js/least/jquery.lazyload.min.js')     }}" type="text/javascript"></script>

        {# JS plantilla #}
        <script src="{{ asset('js/pixel/jquery.jcarousel.js')     }}" type="text/javascript"></script>
        <script src="{{ asset('js/pixel/fancybox/jquery.fancybox-1.3.1.js')}}" type="text/javascript"></script>
        <script src="{{ asset('js/pixel/jquery-func.js')     }}" type="text/javascript"></script>

        {# Script sencillo de least #}
        <script type="text/javascript">
            $(document).ready(function(){
                $('#gallery').least();
            });
        </script>
    </body>
</html>
